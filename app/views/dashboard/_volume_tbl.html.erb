<div class="modal fade " id="exampleModal" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-xl">
    <div class="modal-content modal-view">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Remaining Volume</h5>
        <%= link_to download_excel_remaining_volume_dashboard_index_path(year: @year, month: @month), 
            class: "px-2 btn btn-primary" do %>
          <i class="fas fa-download"></i>
        <% end %>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body scroll">
        <table >
          <thead>
            <tr>
              <th>Shipment</th>
              <th>Date Received</th>
              <th>Type</th>
              <th>State Code</th>
              <th>Rider No</th>
              <th>Coverage Date-From</th>
              <th>Coverage Date-to</th>
              <th>Volume</th>
              <th>Index Unit</th>
              <th>Index Date</th>
              <th>Blank Party-Unit</th>
              <th>Blank Party-Date</th>
              <th>Collateral-unit</th>
              <th>Collateral-date</th>
              <th>Special-Unit</th>
              <th>Special-Date</th>
              <th>Tax Lien-Unit</th>
              <th>Tax Lien-Date</th>
              <th>Frame Scanned-Unit</th>
              <th>Frame Scanned-Date</th>
              <th>MDB-Unit</th>
              <th>MDB-Date</th>
              <th>Office Product-Unit</th>
              <th>Office Product-Date</th>
              <th>TAT-Index</th>
              <th>TAT-Blank Party</th>
              <th>TAT-Collateral</th>
              <th>TAT-Special</th>
              <th>TAT-Tax Lien</th>
              <th>TAT-MDB</th>
              <th>TAT-Office Product</th>
            </tr>
          </thead>
          <tbody>
            <% @default_table.each do |row| %>
              <tr>
                <td><%= row['shipment'] %></td>
                <td><%= row['date_received'] %></td>
                <td><%= row['type'] %></td>
                <td><%= row['state_code'] %></td>
                <td><%= row['rider_no'] %></td>
                <td><%= row['coverage_date_from'] %></td>
                <td><%= row['coverage_date_to'] %></td>
                <td><%= row['volume'] %></td>
                <td><%= row['index_unit'] %></td>
                <td><%= row['index_date'] %></td>
                <td><%= row['blank_party_unit'] %></td>
                <td><%= row['blank_party_date'] %></td>
                <td><%= row['collateral_unit'] %></td>
                <td><%= row['collateral_date'] %></td>
                <td><%= row['special_unit'] %></td>
                <td><%= row['special_date'] %></td>
                <td><%= row['tax_lien_unit'] %></td>
                <td><%= row['tax_lien_date'] %></td>
                <td><%= row['frame_scanned_unit'] %></td>
                <td><%= row['frame_scanned_date'] %></td>
                <td><%= row['mdb_unit'] %></td>
                <td><%= row['mdb_date'] %></td>
                <td><%= row['office_product_unit'] %></td>
                <td><%= row['office_product_date'] %></td>
                <td><%= row['tat_index'] %></td>
                <td><%= row['tat_blank_party'] %></td>
                <td><%= row['tat_collateral'] %></td>
                <td><%= row['tat_special'] %></td>
                <td><%= row['tat_tax_lien'] %></td>
                <td><%= row['tat_mdb'] %></td>
                <td><%= row['tat_office_product'] %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>